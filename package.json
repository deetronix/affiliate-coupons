    "header": {
      "bugs": "https://wordpress.org/support/plugin/affiliate-coupons",
      "team": "Affiliate Coupons <support@affcoups.com>",
      "last_translator": "Affiliate Coupons <support@affcoups.com>"
    }
  },
  "dependencies": {
    "clipboard": "^2.0.6",
    "core-js": "^3.8.3",
    "dotsunited-equal-height-blocks": "3.1.0",
    "gee.less": "3.0.0"
  },
  "devDependencies": {
    "laravel-mix": "^6.0.10",
    "less": "^4.1.0",
    "less-loader": "^7.2.1",
    "postcss": "^8.2.4"
  }
